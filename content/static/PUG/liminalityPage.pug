extends default

include ./liminalityCss.pug
include ./liminalityCover.pug
include ./liminalityContent.pug
include ./footer.pug

block append head
    link(rel = 'stylesheet', href=`/scripts/css/main.css`)
    meta(property="og:type" content="webpage")
    meta(property="og:image" content="http://www.timsokc.com/images/TimsOKC/liminality/liminality.png")
    meta(property="og:image:type" content="image/png")
    meta(property="og:image:width" content="268")
    meta(property="og:image:height" content="268")
    meta(property="og:title" content="Liminality: A Case for Christian Engagement")
    meta(property="og:url" content="http://www.timsokc.com/liminality")
    meta(property="og:description" content="Thoughts on living in the between time.")

append content
    +liminalityCss
    +liminalityCover
    +liminalityContent
    +footer
